		</scheduler>
		<ee:transform doc:name="Transform Message" doc:id="1efec9f5-da72-458c-a59a-d438619b619b" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/java
---
{
}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
	</flow>
